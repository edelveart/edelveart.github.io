---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<section class="mb-10">
  <h2 class="text-2xl font-semibold">{title}</h2>

  <ul class="flex flex-col list-disc ml-4 mt-3 gap-y-2">
    <li>
      <a
        class="text-primary hover:underline"
        href="/resources-files/cheatsheet/figuratenum/figuratenum-python-cheatsheet.pdf"
        title="figuratenum-python-cheatsheet.pdf.pdf"
        target="_blank"
        rel="noopener">FigurateNum Python Cheatsheet - PDF</a
      >
    </li>
  </ul>
</section>

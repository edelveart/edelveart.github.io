---
import BaseLayout from "@layouts/en/BaseLayout.astro";
import ProjectsComponent, { allProjects } from "@components/ProjectsComponent.astro";
---

<BaseLayout title="Projects" sideBarActiveItemID="projects">
  <section class="mt-[-0.75rem] max-w-[700px]">
    <ProjectsComponent headTitle={"Projects"} projects={allProjects} indices={[0, 1, 2, 5, 3, 4]} />
  </section>
</BaseLayout>

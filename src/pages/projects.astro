---
import BaseLayout from "@layouts/BaseLayout.astro";
import ProjectsComponent, {
  allProjects,
} from "@components/ProjectsComponent.astro";
---

<BaseLayout title="Projects" sideBarActiveItemID="projects">
  <section class="mt-[-0.75rem] max-w-[700px]">
    <ProjectsComponent
      headTitle="Proyectos de software"
      projects={allProjects}
      indices={[0, 1, 2, 3, 4]}
    />
  </section>
</BaseLayout>
